{
    "name": "coroowicaksono/nova-slider",
    "description": "A Simple Slider Card in Laravel Nova's Dashboard using Vue-Caraosel. Starting create your own dashboard with Nova Slider can save your time and help you maintain consistency across cool elements of your cards.",
    "keywords": [
        "laravel",
        "nova",
        "slider",
        "card"
    ],
    "license": "MIT",
    "homepage": "https://github.com/coroo",
    "authors": [
        {
            "name": "Kuncoro Wicaksono",
            "email": "coroo.wicaksono@gmail.com",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Coroowicaksono\\NovaSlider\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coroowicaksono\\NovaSlider\\CardSliderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
