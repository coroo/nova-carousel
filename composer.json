{
    "name": "coroowicaksono/nova-carousel",
    "description": "A Simple Slider Card in Laravel Nova's Dashboard using Vue-Caraosel. Starting create your own dashboard with Nova Carousel can save your time and help you maintain consistency across cool elements of your cards.",
    "keywords": [
        "laravel",
        "nova",
        "slider",
        "carousel",
        "card"
    ],
    "license": "MIT",
    "homepage": "https://github.com/coroo",
    "authors": [
        {
            "name": "Kuncoro Wicaksono",
            "email": "coroo.wicaksono@gmail.com",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Coroowicaksono\\NovaCarousel\\": "src/",
            "Coroowicaksono\\NovaCarousel\\Api\\": "src/api"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coroowicaksono\\NovaCarousel\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
